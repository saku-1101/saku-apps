{
  "name": "git-green",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules && rimraf .turbo",
    "check:format": "bunx biome format --changed --write .",
    "check:lint": "bunx biome lint --changed --apply .",
    "check:typecheck": "tsc --noEmit",
    "check:lint-format": "bunx biome check --changed --apply .",
    "check": "bun run check:typecheck & bunx biome check --apply . --no-errors-on-unmatched",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.5.0",
    "@storybook/addon-essentials": "8.1.9",
    "@storybook/addon-interactions": "8.1.9",
    "@storybook/addon-links": "8.1.9",
    "@storybook/addon-onboarding": "8.1.9",
    "@storybook/blocks": "8.1.9",
    "@storybook/react": "8.1.9",
    "@storybook/react-vite": "8.1.9",
    "@storybook/test": "8.1.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-storybook": "0.8.0",
    "storybook": "8.1.9",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": "21.7.3"
  },
  "volta": {
    "node": "21.7.3"
  }
}
