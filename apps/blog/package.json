{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:unit": "jest --config jest.config.ts",
    "test:update": "jest --config jest.config.ts --updateSnapshot",
    "test:watch": "jest --config jest.config.ts --watchAll",
    "test:debug": "jest --config jest.config.ts --runInBand",
    "test": "bun run test:unit",
    "check:format": "bunx biome format --changed --write .",
    "check:lint": "bunx biome lint --changed --apply .",
    "check:typecheck": "tsc --noEmit",
    "check:lint-format": "bunx biome check --changed --apply .",
    "check": "bun run check:typecheck & bunx biome check --apply . --no-errors-on-unmatched",
    "gen:co": "cf-content-types-generator --v10 -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_MANAGEMENT_API_ACCESS_TOKEN -o ../../@types/contentful",
    "setup": "./bin/setup.sh"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.19.4",
    "@repo/ui": "*",
    "cf-content-types-generator": "^2.14.0",
    "contentful": "^10.6.21",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "^16.3.4",
    "@next/eslint-plugin-next": "^14.0.4",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
