{
    "name": "blog",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test:unit": "jest --config jest.config.ts",
        "test:update": "jest --config jest.config.ts --updateSnapshot",
        "test:watch": "jest --config jest.config.ts --watchAll",
        "test:debug": "jest --config jest.config.ts --runInBand",
        "test": "bun run test:unit",
        "check:format": "bunx biome format --write .",
        "check:lint": "bunx biome lint .",
        "check:typecheck": "tsc --noEmit",
        "check:lint-format": "bunx biome check .",
        "check": "bun run check:typecheck & bunx @biomejs/biome check --apply  **/*.ts **/*.tsx",
        "ui:gen": "npx hygen ui new",
        "cmp:gen": "npx hygen cmp new",
        "storybook": "EXPO_PUBLIC_STORYBOOK_ENABLED='true' storybook dev -p 6006",
        "storybook:generate": "sb-rn-get-stories",
        "storybook:watch": "sb-rn-watcher",
        "build:storybook": "storybook build"
    },
    "dependencies": {
        "@repo/ui": "*",
        "next": "^14.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.0.4",
        "@repo/biome-config": "*",
        "@repo/typescript-config": "*",
        "@types/node": "^20.10.6",
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.18",
        "typescript": "^5.3.3"
    }
}
